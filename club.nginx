server {
    listen 80;

    location /images {
        alias /var/www/club/images;
    }

    location /static {
        alias /var/www/club/static;
    }

    location /club {
        proxy_pass http://localhost:2000;
    }

    location /weibo {
        proxy_pass http://localhost:3000;
    }

    location / {
        proxy_pass http://localhost:5000;
    }
}
